pragma.syntax("0.9")

def makeBotFunc (arenaMaxLoc, logFile) {
    def botFunc(myLoc, myVel, enemyLoc, enemyVel) {
        if (myLoc == 0) {
            return 1
        } else if (myLoc == arenaMaxLoc) {
            return -1
        } else {return myVel}
    }
    return botFunc
}
