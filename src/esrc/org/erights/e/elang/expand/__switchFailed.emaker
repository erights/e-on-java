pragma.syntax("0.9")

# XXX in E-on-CL this is a user-defined exception type, which E-on-Java doesn't
# support yet
def makeSwitchFailure := <elang:expand.makeSwitchFailure>

def switchFailed { # implements DeepFrozen
    match [=="run", [specimen] + failures] {
        throw(makeSwitchFailure(specimen, failures))
    }
}